class t{constructor(){this.requestIdCounter=0}async request(t){const s=this.requestIdCounter;this.requestIdCounter++;const e=await window.ipc.request({requestId:s,channel:t.channel,action:t.action,payload:t.payload});return(r=e,JSON.parse(r,((t,s)=>"string"==typeof s&&s.startsWith("PrismaBigInt::")?BigInt(s.substr("PrismaBigInt::".length)):"string"==typeof s&&s.startsWith("PrismaBytes::")?s.substr("PrismaBytes::".length):s))).payload;var r}}export{t as ElectronBrowserTransport};
