{
  "name": "@backframe/admin",
  "version": "0.0.0",
  "description": "The backframe admin package",
  "main": "index.js",
  "homepage": "/admin",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node index.js",
    "build": "tsc && pnpm build:client && pnpm build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node index.js",
    "prebuild": "rimraf dist"
  },
  "dependencies": {
    "@backframe/core": "workspace:^0.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.3",
    "@tabler/icons": "^1.101.0",
    "chart.js": "^3.9.1",
    "clsx": "^1.2.1",
    "d3": "^7.6.1",
    "express": "^4.18.2",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-icons": "^4.4.0",
    "react-leaflet": "^4.1.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.4.2",
    "yum": "^0.1.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-leaflet": "^2.8.2",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.17",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
