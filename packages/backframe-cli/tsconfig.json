{
  "extends": "../../tsconfig.shared.json",
  "compilerOptions": {
    "types": ["node"],
    "target": "es5",
    "lib": ["esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    // "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "outDir": "./dist"
    // "rootDir": "./src"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
