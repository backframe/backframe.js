{
  "name": "@backframe/cli",
  "version": "0.0.0-alpha.7",
  "description": "Backframe command line interface",
  "type": "module",
  "license": "MIT",
  "bin": {
    "bf": "dist/index.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "prebuild": "rimraf dist",
    "lint": "eslint .",
    "fmt": "prettier --write ./src/**/*.ts",
    "lint:fix": "eslint . --fix",
    "lint:fmt": "prettier --check ./src/**/*.ts"
  },
  "dependencies": {
    "@babel/generator": "^7.20.7",
    "@babel/parser": "^7.20.7",
    "@babel/traverse": "^7.20.12",
    "@babel/types": "^7.20.7",
    "@backframe/create-bf": "workspace:*",
    "@backframe/models": "workspace:*",
    "@backframe/utils": "workspace:*",
    "chokidar": "^3.5.3",
    "commander": "^10.0.0",
    "consola": "^2.15.3",
    "cross-spawn": "^7.0.3",
    "esbuild": "^0.16.12",
    "get-pm": "^0.0.1",
    "globby": "^13.1.2",
    "kleur": "^4.1.5",
    "prompts": "^2.4.2",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@backframe/core": "workspace:*",
    "@backframe/eslint-config": "workspace:*",
    "@backframe/rest": "workspace:*",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/prompts": "^2.4.1",
    "@types/yargs": "^17.0.15",
    "unbuild": "^1.0.2"
  },
  "eslintConfig": {
    "extends": "@backframe/eslint-config"
  },
  "eslintIgnore": [
    "dist/"
  ]
}
