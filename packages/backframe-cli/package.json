{
  "name": "@backframe/cli",
  "version": "0.0.0-alpha.0",
  "description": "Backframe command line interface",
  "type": "module",
  "license": "MIT",
  "bin": {
    "bf": "dist/index.mjs"
  },
  "publishConfig": {
