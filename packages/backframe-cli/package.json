    "prompts": "^2.4.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@backframe/core": "workspace:*",
    "@backframe/rest": "workspace:*",
    "@types/cross-spawn": "^6.0.2",
    "@types/prompts": "^2.4.1",
    "@types/yargs": "^17.0.15",
    "unbuild": "^1.0.2"
